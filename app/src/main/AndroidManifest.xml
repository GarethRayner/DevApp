<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycompany.devapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ArticleRead"
            android:label="@string/article_label"
            android:parentActivityName=".HomeScreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mycompany.devapp.HomeScreen" />
 >
        </activity>
        <activity
            android:name=".ArticleSlider"
            android:label="@string/title_activity_article_slider"
            android:parentActivityName=".HomeScreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mycompany.devapp.HomeScreen" />
        </activity>
        <activity
            android:name=".NewArt"
            android:label="@string/title_activity_new_art"
            android:parentActivityName=".HomeScreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mycompany.devapp.HomeScreen" />
        </activity>
        <activity
            android:name=".NewArtRead"
            android:label="@string/title_activity_new_art_read"
            android:parentActivityName=".NewArt" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mycompany.devapp.NewArt" />
        </activity>
        <activity
            android:name=".Notes"
            android:label="@string/title_activity_notes"
            android:parentActivityName=".HomeScreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mycompany.devapp.HomeScreen" />
        </activity>
    </application>

</manifest>
